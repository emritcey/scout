{
  "manifest_version": 2,
  "name": "Scout",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.0.0.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }, 
  "background": {
    "scripts": ["background.js"]
  }
}
